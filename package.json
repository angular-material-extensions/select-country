{
  "name": "select-country",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4901",
    "build": "ng build",
    "build:client-and-server-bundles": "ng build --prod && ng run select-country:server:production --bundleDependencies all",
    "build:client-and-server-bundles:ghpages": "npm run build:prod:ghpages && ng run select-country:server:production --bundleDependencies all",
    "build:docs": "npx compodoc -p projects/angular-material-extensions/select-country/tsconfig.lib.json --hideGenerator --disableCoverage -d dist/browser/doc/",
    "build:lib": "ng build @angular-material-extensions/select-country",
    "build:lib:Watch": "ng build @angular-material-extensions/select-country --watch",
    "build:prod": "ng build --prod",
    "build:prod:ghpages": "ng build --preserve-symlinks --prod --aot --build-optimizer --base-href /select-country/ --deploy-url /select-country/",
    "build:prerender": "npm run build:client-and-server-bundles && npm run compile:server && npm run generate:prerender",
    "build:prerender:ghpages": "npm run build:client-and-server-bundles:ghpages && npm run compile:server && npm run generate:prerender",
    "deploy": "ng deploy --base-href=/select-country/ --no-build",
    "generate:prerender": "cd dist && node prerender",
    "test": "jest",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "compile:server": "webpack --config webpack.server.config.js --progress --colors",
    "serve:prerender": "cd dist/browser && http-server",
    "serve:ssr": "node dist/server",
    "build:ssr": "npm run build:client-and-server-bundles && npm run compile:server",
    "release": "release-it",
    "test:watch": "jest --watch"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.14",
    "@angular/cdk": "~8.2.3",
    "@angular/common": "^8.2.14",
    "@angular/compiler": "^8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/flex-layout": "^8.0.0-beta.27",
    "@angular/forms": "^8.2.14",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "@angular/platform-server": "^8.2.14",
    "@angular/router": "^8.2.14",
    "@briebug/jest-schematic": "^2.1.1",
    "@nguniversal/express-engine": "^8.2.6",
    "@nguniversal/module-map-ngfactory-loader": "8.1.1",
    "angular-cli-ghpages": "^0.6.2",
    "angulartics2": "^8.3.0",
    "express": "^4.15.2",
    "hammerjs": "^2.0.8",
    "ngx-markdown": "^8.2.1",
    "rxjs": "^6.5.4",
    "svg-country-flags": "^1.2.7",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-builders/jest": "^8.3.2",
    "@angular-devkit/build-angular": "^0.803.23",
    "@angular-devkit/build-ng-packagr": "^0.803.23",
    "@angular-devkit/schematics": "^8.3.23",
    "@angular/cli": "^8.3.23",
    "@angular/compiler-cli": "^8.2.14",
    "@angular/language-service": "^8.2.14",
    "@compodoc/compodoc": "^1.1.11",
    "@release-it/conventional-changelog": "^1.1.0",
    "@schematics/angular": "^8.3.23",
    "@schematics/update": "^0.803.23",
    "@types/jasmine": "~3.4.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/jest": "^24.9.0",
    "@types/node": "^12.12.25",
    "codecov": "^3.6.1",
    "codelyzer": "^5.2.1",
    "http-server": "^0.11.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "jest": "^24.9.0",
    "jest-preset-angular": "^8.0.0",
    "ng-packagr": "^5.4.0",
    "protractor": "~5.4.0",
    "release-it": "^12.4.3",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tsickle": "^0.37.0",
    "tslint": "~5.20.1",
    "typescript": "~3.5.3",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  },
  "greenkeeper": {
    "ignore": [
      "@angular/animations",
      "@angular/core",
      "@angular/common",
      "@angular/compiler",
      "@angular/cdk",
      "@angular/material",
      "@angular/platform-server",
      "@angular/platform-browser",
      "@angular/platform-browser-dynamic",
      "@angular/compiler-cli",
      "webpack",
      "zone.js",
      "rxjs",
      "tslint",
      "typescript",
      "codelyzer",
      "@types/jasmine",
      "@types/jest",
      "@types/node"
    ]
  },
  "jest": {
    "preset": "jest-preset-angular",
    "roots": [
      "src"
    ],
    "transform": {
      "^.+\\.(ts|js|html)$": "ts-jest"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/src/setup-jest.ts"
    ],
    "moduleNameMapper": {
      "@app/(.*)": "<rootDir>/src/app/$1",
      "@assets/(.*)": "<rootDir>/src/assets/$1",
      "@core/(.*)": "<rootDir>/src/app/core/$1",
      "@env": "<rootDir>/src/environments/environment",
      "@src/(.*)": "<rootDir>/src/src/$1",
      "@state/(.*)": "<rootDir>/src/app/state/$1"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "<rootDir>/tsconfig.spec.json",
        "stringifyContentPathRegex": "\\.html$",
        "astTransformers": [
          "jest-preset-angular/build/InlineFilesTransformer",
          "jest-preset-angular/build/StripStylesTransformer"
        ]
      }
    }
  }
}
